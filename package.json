{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React starter boilerplate",
  "main": "src/index.jsx",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "check": "tsc --noEmit",
    "develop": "vite",
    "fix": "npm run fix:css && npm run fix:js",
    "fix:css": "npm run lint:css -- --fix",
    "fix:js": "npm run lint:js -- --fix",
    "format": "prettier --write .",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint '**/*.css'",
    "lint:js": "eslint '**/*.{js,jsx,ts,tsx}'",
    "preview": "vite preview",
    "test": "jest",
    "test:ci": "npm test && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcobiedermann/react-boilerplate.git"
  },
  "keywords": [
    "React",
    "JavaScript",
    "Boilerplate"
  ],
  "author": "Marco Biedermann",
  "bugs": {
    "url": "https://github.com/marcobiedermann/react-boilerplate/issues"
  },
  "license": "MIT",
  "dependencies": {
    "i18next": "^21.6.16",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.16.7",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react-dom": "^17.0.0",
    "@types/react": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vitejs/plugin-react": "^1.3.0",
    "codecov": "^3.8.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^17.0.1",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.9.5"
  }
}
