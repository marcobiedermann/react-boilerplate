{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React starter boilerplate",
  "main": "src/index.html",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "check": "tsc --noEmit",
    "develop": "vite",
    "fix": "npm run fix:css && npm run fix:js",
    "fix:css": "npm run lint:css -- --fix",
    "fix:js": "npm run lint:js -- --fix",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint --ignore-path .gitignore 'src/**/*.css'",
    "lint:js": "eslint --ignore-path .gitignore 'src/**/*.{js,jsx,ts,tsx}'",
    "preview": "vite preview",
    "test": "jest --maxWorkers=50% --watch",
    "test:ci": "jest --ci --coverage",
    "server": "json-server --watch db.json --port 8080",
    "update": "npx npm-check-updates --format group --interactive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcobiedermann/react-boilerplate.git"
  },
  "keywords": [
    "React",
    "JavaScript",
    "Boilerplate"
  ],
  "author": "Marco Biedermann",
  "bugs": {
    "url": "https://github.com/marcobiedermann/react-boilerplate/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@tanstack/react-query": "^5.17.1",
    "@tanstack/react-query-devtools": "^5.17.1",
    "axios": "^1.6.4",
    "date-fns": "^3.1.0",
    "i18next": "^21.8.16",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.49.2",
    "react-i18next": "^11.18.3",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.4.2"
  },
  "devDependencies": {
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.47",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "json-server": "^0.17.0",
    "msw": "^0.44.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.18"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
