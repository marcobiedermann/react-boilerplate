{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React starter boilerplate",
  "main": "client/index.jsx",
  "scripts": {
    "build": "webpack --config webpack.production.config.js",
    "develop": "webpack-dev-server --config webpack.development.config.js",
    "lint:css": "stylelint '**/*.css'",
    "lint:js": "eslint '**/*.{js,jsx}'",
    "lint": "npm run lint:css && npm run lint:js",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcobiedermann/react-boilerplate.git"
  },
  "keywords": [
    "React",
    "JavaScript",
    "Boilerplate"
  ],
  "author": "Marco Biedermann",
  "bugs": {
    "url": "https://github.com/marcobiedermann/react-boilerplate/issues"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "babel-loader": "^8.0.0-beta.6",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "dotenv": "^6.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^4.0.0-alpha",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.22.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  }
}
